
  %set OUTPUT_NAME hello

  # New source code module to be compiled
  CONFIG_MODULES +$(OUTPUT_NAME):$(CONFIGPATH)

  # Application license
  CONFIG_LICENSE_APP_LGPL

  # Mutek features
  CONFIG_MUTEK_CONSOLE
  CONFIG_MUTEK_SCHEDULER_MIGRATION
  CONFIG_MUTEK_MEMALLOC_SMART

  # Libs
  CONFIG_PTHREAD

%include ../common/build_options.conf
%include ../common/platforms.conf

