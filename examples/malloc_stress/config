
%common		# optional at file start

  # New source code module to be compiled
  CONFIG_MODULES app:$(CONFIGPATH)

  # Application license
  CONFIG_LICENSE_APP_LGPL

  # Mutek features
  CONFIG_MUTEK_CONSOLE
  CONFIG_MUTEK_SCHEDULER_MIGRATION
  CONFIG_MUTEK_MEMALLOC_SMART

  # Libs
  CONFIG_PTHREAD
  CONFIG_LIBCRYPTO_CRC32


%section default		# default build section name

  %error Missing build configuration name, please specify BUILD=xxx

%common

%include ../common/platforms.conf
%include ../common/build_options.conf
