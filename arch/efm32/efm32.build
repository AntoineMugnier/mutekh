  %subsection efm32-wgNNN-fX-* efm32-lgNNN-fX-*
    %types sram

    CONFIG_HEXO_CPU_STACK_SIZE 0x400
    CONFIG_PTHREAD_STACK_SIZE  0x800

    CONFIG_EFM32_RAMSIZE 0x8000

    %subsection efm32-*-f64-*
      %types flash
      CONFIG_EFM32_FLASHSIZE 0x00010000
