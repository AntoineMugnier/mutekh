
%config CONFIG_DRIVER_PIC32_DMA
  desc Enable PIC32 dma driver
  parent CONFIG_ARCH_PIC32
  depend CONFIG_DEVICE_DMA
  require CONFIG_DRIVER_PIC32_DMA_CHANNEL_COUNT
%config end

%config CONFIG_DRIVER_PIC32_DMA_CHANNEL_COUNT
  desc Number of dma channel used
  parent CONFIG_DRIVER_PIC32_DMA
  flags value
  default 8
%config end
