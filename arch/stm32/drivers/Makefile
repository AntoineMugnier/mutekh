
ifeq ($(CONFIG_DRIVER_STM32_USART), defined)
subdirs += usart
endif

ifeq ($(CONFIG_DRIVER_STM32_I2C), defined)
subdirs += i2c
endif

ifeq ($(CONFIG_DRIVER_STM32_GPIO), defined)
subdirs += gpio
endif

ifeq ($(CONFIG_DRIVER_STM32_RCC), defined)
subdirs += rcc
endif

ifeq ($(CONFIG_DRIVER_STM32_PWM), defined)
subdirs += pwm
endif

